{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "yarn install",
        "buildCommand": "SHARP_IGNORE_GLOBAL_LIBVIPS=1 yarn build"
      }
    }
  ],
  "env": {
    "SHARP_IGNORE_GLOBAL_LIBVIPS": "1"
  },
  "redirects": [
    {
      "source": "/pages/voices.html",
      "destination": "https://archive.nadinicoco.com/pages/voices.html",
      "permanent": true
    }
  ]
} 